{
  "extends": [
    "config:base",
    ":prHourlyLimitNone",
    ":rebaseStalePrs"
  ],
  "ignorePaths": [
    "docs/**"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        "^io.quarkus:"
      ],
      "groupName": "io.quarkus"
    },
    {
      "packagePatterns": [
        "^org.junit.jupiter:junit-jupiter-api:",
        "^org.junit.jupiter:junit-jupiter-engine:"
      ],
      "groupName": "org.junit.jupiter"
    },
    {
      "packagePatterns": [
        "^org.seasar.doma:"
      ],
      "groupName": "org.seasar.doma"
    }
  ]
}
