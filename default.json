{
  "assignAutomerge": true,
  "extends": [
    "config:base",
    ":automergeMinor",
    ":dependencyDashboard",
    ":prHourlyLimitNone",
    ":rebaseStalePrs"
  ],
  "ignorePaths": [
    "docs/**"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "groupName": "io.quarkus",
      "packagePatterns": [
        "^io.quarkus:"
      ]
    },
    {
      "groupName": "org.jetbrains.kotlin",
      "packagePatterns": [
        "^org.jetbrains.kotlin"
      ]
    },
    {
      "groupName": "org.junit.jupiter",
      "packagePatterns": [
        "^org.junit.jupiter:"
      ]
    },
    {
      "groupName": "org.seasar.doma",
      "packagePatterns": [
        "^org.seasar.doma:(core|mock|kotlin|processor)"
      ]
    },
    {
      "groupName": "org.seasar.doma:doma-quarkus",
      "packagePatterns": [
        "^org.seasar.doma:doma-quarkus"
      ]
    }
  ],
  "reviewers": [
    "nakamura-to"
  ]
}
